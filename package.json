{
  "name": "Overleaf",
  "version": "1.0.1",
  "description": "A dedicated desktop application for Overleaf, the online LaTeX editor. Written in electron and built in electron-builder.",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "build": "sh build.sh"
  },
  "keywords": [
    "Overleaf",
    "LaTex"
  ],
  "author": "oitsjustjose",
  "license": "CC0-1.0",
  "devDependencies": {
    "electron": "^4.1.3",
    "electron-builder": "^20.39.0"
  },
  "dependencies": {
    "electron-context-menu": "^0.12.1",
    "electron-log": "^3.0.5",
    "element-ready": "^3.1.0"
  },
  "build": {
    "appId": "com.oitsjustjose.overleafdesktop",
    "appImage": {
      "category": "public.app-category.productivity"
    },
    "mac": {
      "category": "public.app-category.productivity",
      "icon": "assets/icons/macOS/overleaf.icns"
    },
    "win": {
      "icon": "assets/icons/win/overleaf.ico"
    },
    "linux": {
      "icon": "assets/icons/png/overleaf.png"
    },
    "deb": {
      "icon": "assets/icons/png/overleaf.png"
    },
    "snap": {},
    "pacman": {
      "icon": "assets/icons/png/overleaf.png"
    },
    "rpm": {
      "icon": "assets/icons/png/overleaf.png"
    },
    "freebsd": {
      "icon": "assets/icons/png/overleaf.png"
    },
    "p5p": {
      "icon": "assets/icons/png/overleaf.png"
    },
    "apk": {
      "icon": "assets/icons/png/overleaf.png"
    }
  }
}